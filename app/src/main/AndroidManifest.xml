<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ungdungweb_demo">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".View.DoanhThuActivity" />
        <activity android:name=".View.AddMonActivity" />
        <activity android:name=".View.MenuActivity" />
        <activity android:name=".View.RequestActivity" />
        <activity android:name=".XacNhanDonHangFinal" />
        <activity android:name=".XacNhanDonHang" />
        <activity android:name=".QuanLy" />
        <activity android:name=".Coupon" />
        <activity android:name=".TichDiem" />
        <activity android:name=".View.HelpActivity" />
        <activity android:name=".View.LoginActivity" />
        <activity android:name=".View.SignupActivity" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name=".View.MapsActivity"
            android:label="@string/title_activity_maps" />
        <activity android:name=".Detail_Info" />

        <uses-library android:name="com.google.android.maps" />

        <activity android:name=".View.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>